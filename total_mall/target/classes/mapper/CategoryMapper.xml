<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="human.smart.totalMall.CategoryMapper">

	<!-- 하나의 게시글 조회: select -->
	<select id="getCategory" parameterType="CategoryVO">
		select * from kategory where k_idx = #{k_idx} and kategory_name = #{kategory_name}
	</select>

</mapper>